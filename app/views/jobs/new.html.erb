
<!-- collection_select(:post, :category_id, Category.all, :id, :name, -->
<div>
  <%= form_for @job do |f| %>
    <%= f.label "What industry are you in?" %>
  <%= collection_select(:job, :industry_id, Industry.all, :id, :classification) %>
    <%= f.label "Select your College or University" %>
      <%= f.text_field :college %>
    <%= f.label "Your Monthly Student Loan Debt Amount" %>
      <%=  f.number_field :student_loan_pmt %>
    <%= f.label "What kind of job title best represents you?" %>
      <%= f.collection_select :job_title, BlsComponent.all, :title, :title %>
    <%= f.submit "Create" %>
  <% end %>
</div>

